<?xml version="1.0"?>
<schema xmlns="http://www.w3.org/2000/10/XMLSchema">
    <element name="ID">
        <complexType>
            <attribute name="type" use="optional">
                <simpleType>
                    <restriction base="string">
                        <enumeration value="patient ID"/>
                        <enumeration value="case ID"/>
                        <enumeration value="social security number"/>
                        <enumeration value="medical insurance number"/>
                        <enumeration value="other"/>
                    </restriction>
                </simpleType>
            </attribute>
            <attribute name="rule" use="optional">
                <simpleType>
                    <restriction base="string"/>
                </simpleType>
            </attribute>
            <attribute name="confidence" use="optional">
                <simpleType>
                    <restriction base="float"/>
                </simpleType>
            </attribute>
        </complexType>
    </element>
</schema>
